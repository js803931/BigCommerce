{"components/common/cart-preview":"<div class=\"cart-dropdown-arrow\"></div>\r\n<div class=\"cart-dropdown-arrow-border\"></div>\r\n<div class=\"previewCart\">\r\n    {{#if cart.items.length}}\r\n        <ul class=\"previewCartList\">\r\n            {{#each (limit cart.items 4)}}\r\n                <li class=\"previewCartItem\">\r\n                    <div class=\"previewCartItem-image\">\r\n                        {{#if type '==' 'GiftCertificate'}}\r\n                            <img class=\"lazyload\" data-sizes=\"auto\" src=\"{{cdn 'img/loading.svg'}}\" data-src=\"{{cdn ../../theme_settings.default_image_gift_certificate}}\" alt=\"GiftCertificate\" title=\"GiftCertificate\">\r\n                        {{else}}\r\n                            <img class=\"lazyload\" data-sizes=\"auto\" src=\"{{cdn 'img/loading.svg'}}\" data-src=\"{{getImage image 'productthumb_size' (cdn ../../theme_settings.default_image_product)}}\" alt=\"{{image.alt}}\" title=\"{{image.alt}}\">\r\n                        {{/if}}\r\n                    </div>\r\n\r\n                    <div class=\"previewCartItem-content\">\r\n                        {{#if brand.name}}\r\n                            <span class=\"previewCartItem-brand\">\r\n                                {{brand.name}}\r\n                            </span>\r\n                        {{/if}}\r\n\r\n                        <h6 class=\"previewCartItem-name\">\r\n                            <a href=\"{{url}}\" alt=\"{{name}}\" title=\"{{name}}\">{{name}}</a>\r\n                        </h6>\r\n\r\n                        {{#if options}}\r\n                        <dl class=\"definitionList cart__header-list\">\r\n                            {{#each options}}\r\n                            <dt class=\"definitionList-key cart__header-list-key\">{{name}}:</dt>\r\n                            <dd class=\"definitionList-value cart__header-list-value\">{{value}}</dd>\r\n                            {{/each}}\r\n                            <dt class=\"previewCartItem-quantity definitionList-key cart__header-list-key\">Qty:</dt>\r\n                            <dd class=\"definitionList-value cart__header-list-value cart__header-qty\">{{quantity}}</dd>\r\n                            <dt class=\"definitionList-key cart__header-list-key\">\r\n                                <div class=\"previewCartItem-price\">\r\n                                     <span class=\"cart-preview__rrp-price\">{{rrp.formatted}}</span>    \r\n                                     <span class=\"cart-preview__price\">{{price.formatted}} <span class=\"each-span\">ea.</span></span>\r\n                                </div>\r\n\r\n                                     \r\n                            </dt>\r\n                        </dl>\r\n                        {{/if}}\r\n\r\n                        \r\n                    </div>\r\n                </li>\r\n            {{/each}}\r\n        </ul>\r\n        <div class=\"previewCartAction cart__header-btns\">\r\n            {{#if cart.show_primary_checkout_button}}\r\n                <div class=\"previewCartAction-checkout\">\r\n                    <a href=\"{{urls.checkout.single_address}}\" class=\"button button--small button--primary cart__header-checkout-btn\">\r\n                        {{lang 'cart.preview.checkout_now'}}\r\n                    </a>\r\n                </div>\r\n            {{/if}}\r\n\r\n            <div class=\"previewCartAction-viewCart{{#unless cart.show_primary_checkout_button}} previewCartAction-viewCart--sole{{/unless}} cart__header-view-cart-btn\">\r\n                <a href=\"{{urls.cart}}\" class=\"button button--small button--action\">\r\n                    {{lang 'cart.preview.view_cart'}}\r\n                </a>\r\n            </div>\r\n\r\n            {{#if cart.show_multiple_address_shipping}}\r\n                <div class=\"previewCartAction-checkoutMultiple\">\r\n                    <a href=\"{{urls.checkout.multiple_address}}\">\r\n                        {{lang 'cart.preview.checkout_multiple'}}\r\n                    </a>\r\n                </div>\r\n            {{/if}}\r\n        </div>\r\n    {{else}}\r\n        <div class=\"previewCart-emptyBody\">\r\n            {{lang 'cart.checkout.empty_cart'}}\r\n        </div>\r\n    {{/if}}\r\n</div>\r\n"}

{"components/common/forms/text":"<div class=\"form-field\" id=\"{{id}}\" data-validation=\"{{validation}}\" {{#if private_id}}data-type=\"{{private_id}}\"{{/if}}>\r\n    <label class=\"form-label\" for=\"{{id}}_input\">\r\n        {{#if required}}\r\n           <span class=\"req-asterisk\">*</span>{{label}}:\r\n        {{/if}}\r\n    </label>\r\n    <input type=\"text\" id=\"{{id}}_input\" data-label=\"{{label}}\" name=\"{{name}}\" {{#if value}} value=\"{{value}}\"{{/if}} class=\"form-input\" aria-required=\"{{required}}\" {{#if private_id}}data-field-type=\"{{private_id}}\"{{/if}}>\r\n</div>\r\n"}

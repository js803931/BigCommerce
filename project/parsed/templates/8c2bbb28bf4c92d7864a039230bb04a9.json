{"components/common/subscription-form":"<h5 class=\"footer-info-heading footer__headline\">{{lang 'custom.footer-signup-text'}} <a href=\"##\" class=\"footer__headline-legal-link\">Legal\r\n        Terms</a></h5>\r\n\r\n<form class=\"form\" action=\"{{urls.subscribe.action}}\" method=\"post\">\r\n    <fieldset class=\"form-fieldset\">\r\n        <input type=\"hidden\" name=\"action\" value=\"subscribe\">\r\n        <input type=\"hidden\" name=\"nl_first_name\" value=\"bc\">\r\n        <input type=\"hidden\" name=\"check\" value=\"1\">\r\n        <div class=\"form-field\">\r\n            <label class=\"form-label is-srOnly\" for=\"nl_email\">{{lang 'common.email_address'}}</label>\r\n            <div class=\"form-prefixPostfix wrap\">\r\n                <div class=\"footer__input-wrapper\">\r\n                    <input class=\"form-input footer__name-input\" placeholder=\"Name\" type=\"name\" value=\"{{customer.name}}\">\r\n                    <input class=\"form-input footer__email-input\" id=\"nl_email\" name=\"nl_email\" type=\"email\" value=\"{{customer.email}}\"\r\n                    placeholder=\"Email\">\r\n                </div>\r\n\r\n                <div class=\"footer__submit-wrapper\">\r\n                    <input class=\"button button--primary form-prefixPostfix-button--postfix footer__subscribe-btn\" type=\"submit\"\r\n                    value=\"SUBMIT\">\r\n                </div>\r\n            {{#if settings.show_newsletter_summary }}\r\n            <div class=\"footer-newsletter-summary\">{{settings.newsletter_summary}}</div>\r\n            {{/if}}\r\n        </div>\r\n    </fieldset>\r\n</form>\n"}

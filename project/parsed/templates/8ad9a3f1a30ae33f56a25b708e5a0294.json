{"components/common/footer":"\r\n<footer class=\"footer\" role=\"contentinfo\">\r\n    <div class=\"footer-container\">\r\n\r\n        <article class=\"footer-info-col subscribe-container\" data-section-type=\"newsletterSubscription\">\r\n\r\n            <div class=\"subscribe-flex-container container\">\r\n                {{#if settings.show_newsletter_box}}\r\n                {{> components/common/subscription-form}}\r\n\r\n\r\n                <div class=\"footer__social-media-container\">\r\n                    <a href=\"https://www.facebook.com/northernreflections\">\r\n                        <i class=\"fab fa-facebook-f footer__social-media-icon fa-2x\"></i>\r\n                    </a>\r\n                    <a href=\"https://twitter.com/N_Reflections\">\r\n                        <i class=\"fab fa-twitter footer__social-media-icon fa-2x\"></i>\r\n                    </a>\r\n                    <a href=\"https://www.instagram.com/northernreflections\">\r\n                        <i class=\"fab fa-instagram footer__social-media-icon fa-2x\"></i>\r\n                    </a>\r\n                </div>\r\n                {{/if}}\r\n                {{> components/common/payment-icons}}\r\n            </div>\r\n            <p class=\"subscribe-legal-text\">\r\n                    {{lang 'custom.subscribe-legal-text'}}\r\n            </p>\r\n        </article>\r\n\r\n        {{#if theme_settings.social_icon_placement_bottom '!==' 'bottom_none'}}\r\n        <article class=\"footer-info-col\r\n                footer-info-col--social\r\n                footer-info-col--{{#if theme_settings.social_icon_placement_bottom '===' 'bottom_left'}}left{{else}}right{{/if}}\"\r\n            data-section-type=\"footer-webPages\">\r\n            <h5 class=\"footer-info-heading\">{{lang 'social.connect'}}</h5>\r\n            {{> components/common/social-links}}\r\n        </article>\r\n        {{/if}}\r\n\r\n        <section class=\"footer-info\">\r\n\r\n\r\n            <div class=\"footer__acc-menus\">\r\n\r\n\r\n\r\n                <!-- *******************\r\n                    ACCOUNT COLLAPSIBLE\r\n                    ******************* -->\r\n\r\n                <div class=\"footer__collapse-container\">\r\n                    <a href=\"##\" class=\"footer__collapse-menu footer__acc-menu footer__acc-title\" data-collapsible=\"footer__collapse-account\"\r\n                        data-collapsible-disabled-breakpoint=\"medium\" aria-controls=\"footer__collapse-account\"\r\n                        aria-expanded=\"false\">\r\n                        {{pages.0.name}}\r\n                        <span class=\"footer__nav-menu-plus icon icon--add\">\r\n                            <svg class=\"footer__nav-plus\">\r\n                                <use xlink:href=\"#icon-add\"></use>\r\n                            </svg>\r\n                        </span>\r\n\r\n                        <span class=\"icon icon--remove footer__nav-menu-minus\">\r\n                            <svg class=\"footer__nav-minus\">\r\n                                <use xlink:href=\"#icon-remove\"></use>\r\n                            </svg>\r\n                        </span>\r\n                    </a>\r\n\r\n                    <ul class=\"footer__collapse-list footer__acc-item footer__acc-content\" id=\"footer__collapse-account\"\r\n                        aria-hidden=\"true\">\r\n                        {{#each pages.0.children}}\r\n                        <li>\r\n                            <a href=\"{{this.url}}\" class=\"footer__acc-content-item\">{{ this.name }}</a>\r\n                        </li>\r\n                        {{/each}}\r\n                    </ul>\r\n                </div>\r\n\r\n                <!-- *******************\r\n                    COMPANY COLLAPSIBLE\r\n                    ******************* -->\r\n\r\n                <div class=\"footer__collapse-container\">\r\n                    <a href=\"##\" class=\"footer__collapse-menu footer__acc-menu footer__acc-title\" data-collapsible=\"footer__collapse-company\"\r\n                        data-collapsible-disabled-breakpoint=\"medium\" aria-controls=\"footer__collapse-company\"\r\n                        aria-expanded=\"false\">\r\n                        {{pages.1.name}}\r\n                        <span class=\"footer__nav-menu-plus icon icon--add\">\r\n                            <svg class=\"footer__nav-plus\">\r\n                                <use xlink:href=\"#icon-add\"></use>\r\n                            </svg>\r\n                        </span>\r\n\r\n                        <span class=\"icon icon--remove footer__nav-menu-minus\">\r\n                            <svg class=\"footer__nav-minus\">\r\n                                <use xlink:href=\"#icon-remove\"></use>\r\n                            </svg>\r\n                        </span>\r\n                    </a>\r\n\r\n                    <ul class=\"footer__collapse-list footer__acc-item footer__acc-content\" id=\"footer__collapse-company\"\r\n                        aria-hidden=\"true\">\r\n                        {{#each pages.1.children}}\r\n                        <li>\r\n                            <a href=\"{{this.url}}\" class=\"footer__acc-content-item\">{{ this.name }}</a>\r\n                        </li>\r\n                        {{/each}}\r\n                    </ul>\r\n                </div>\r\n                <!-- *******************\r\n                      HELP COLLAPSIBLE\r\n                    ******************* -->\r\n                <div class=\"footer__collapse-container\">\r\n                    <a href=\"##\" class=\"footer__collapse-menu footer__acc-menu footer__acc-title\" data-collapsible=\"footer__collapse-help\"\r\n                        data-collapsible-disabled-breakpoint=\"medium\" aria-controls=\"footer__collapse-help\"\r\n                        aria-expanded=\"false\">\r\n                        {{pages.2.name}}\r\n                        <span class=\"footer__nav-menu-plus icon icon--add\">\r\n                            <svg class=\"footer__nav-plus\">\r\n                                <use xlink:href=\"#icon-add\"></use>\r\n                            </svg>\r\n                        </span>\r\n\r\n                        <span class=\"icon icon--remove footer__nav-menu-minus\">\r\n                            <svg class=\"footer__nav-minus\">\r\n                                <use xlink:href=\"#icon-remove\"></use>\r\n                            </svg>\r\n                        </span>\r\n                    </a>\r\n\r\n                    <ul class=\"footer__collapse-list footer__acc-item footer__acc-content\" id=\"footer__collapse-help\"\r\n                        aria-hidden=\"true\">\r\n                        {{#each pages.2.children}}\r\n                        <li>\r\n                            <a href=\"{{this.url}}\" class=\"footer__acc-content-item\">{{ this.name }}</a>\r\n                        </li>\r\n                        {{/each}}\r\n                    </ul>\r\n                </div>\r\n\r\n                <div class=\"footer__line\"></div>\r\n\r\n                <div class=\"footer__CMN-image-large\">\r\n                    <a href=\"/cmn\">\r\n                        <img class=\"lazyload footer__CMN-large-img\" data-src=\"{{cdn 'webdav:/images/CMN-large-logo.png'}}\" src=\"{{cdn 'img/loading.svg'}}\" alt=\"Children's Miracle Network\">\r\n                        <p class=\"footer__CMN-proud-text\">{{lang 'custom.proud-supporter'}}</p>\r\n                    </a>\r\n                </div>\r\n\r\n            </div>\r\n\r\n            <div class=\"footer__CMN-image-mobile\">\r\n                <a href=\"/cmn\">\r\n                    <img class=\"lazyload footer__CMN-mobile-img\" data-src=\"{{cdn 'webdav:/images/CMN-mobile-logo.png'}}\" src=\"{{cdn 'img/loading.svg'}}\" alt=\"Children's Miracle Network\">\r\n                    <p class=\"footer__CMN-proud-text\">{{lang 'custom.proud-supporter'}}</p>\r\n                </a>\r\n            </div>\r\n\r\n            {{#if settings.address}}\r\n            <article class=\"footer-info-col footer-info-col--small footer__company-payment-info\" data-section-type=\"storeInfo\">\r\n                <!-- <h5 class=\"footer-info-heading\">{{lang 'footer.info'}}</h5> -->\r\n\r\n                <div class=\"footer__company-info\">\r\n\r\n                    <span class=\"icon icon--location-arrow\">\r\n                        <svg>\r\n                            <use xlink:href=\"#icon-location-arrow-solid\"></use>\r\n                        </svg>\r\n                    </span>\r\n\r\n                    <p class=\"footer__company-name\">{{moment format=\"YYYY\"}} {{settings.store_name}} </p>\r\n                    <address class=\"footer__address\">\r\n                        {{settings.address}}\r\n                    </address>\r\n                    {{#if settings.phone_number}}\r\n                    <span class=\"icon icon--phone\">\r\n                        <svg class=\"footer__phone-icon\">\r\n                            <use xlink:href=\"#icon-phone-solid\"></use>\r\n                        </svg>\r\n                    </span>\r\n\r\n                    <p class=\"footer__phone-number\">{{lang 'custom.footer-phone-number'}}</p>\r\n\r\n                </div>\r\n\r\n                <div class=\"footer__payment-icons\">\r\n                    <span class=\"footer__payment-image\">\r\n                            <img class=\"lazyload\" data-src=\"{{cdn 'webdav:/images/paypal-image.png'}}\" src=\"{{cdn 'img/loading.svg'}}\" alt=\"paypal\">\r\n                    </span>\r\n                    <span class=\"footer__payment-image\">\r\n                            <img class=\"lazyload\" data-src=\"{{cdn 'webdav:/images/visa-image.png'}}\" src=\"{{cdn 'img/loading.svg'}}\" alt=\"visa\">\r\n                    </span>\r\n                    <span class=\"footer__payment-image\">\r\n                            <img class=\"lazyload\" data-src=\"{{cdn 'webdav:/images/mastercard-image.png'}}\" src=\"{{cdn 'img/loading.svg'}}\" alt=\"mastercard\">\r\n                    </span>\r\n                    <span class=\"footer__payment-image\">\r\n                            <img class=\"lazyload\" data-src=\"{{cdn 'webdav:/images/amex-image.png'}}\" src=\"{{cdn 'img/loading.svg'}}\" alt=\"amex\">\r\n                    </span>\r\n                    <span class=\"footer__payment-image\">\r\n                            <img class=\"lazyload\" data-src=\"{{cdn 'webdav:/images/geotrust-image.png'}}\" src=\"{{cdn 'img/loading.svg'}}\" alt=\"geotrust\">\r\n                    </span>\r\n                    <span class=\"footer__payment-image ssl-secured\">\r\n                            <img class=\"lazyload\" data-src=\"{{cdn 'webdav:/images/ssl-image.png'}}\" src=\"{{cdn 'img/loading.svg'}}\" alt=\"ssl\">\r\n                    </span>\r\n                </div>\r\n\r\n                {{/if}}\r\n            </article>\r\n            {{/if}}\r\n        </section>\r\n    </div>\r\n</footer>\r\n","components/common/subscription-form":"<h5 class=\"footer-info-heading footer__headline\">{{lang 'custom.footer-signup-text'}} <a href=\"##\" class=\"footer__headline-legal-link\">Legal\r\n        Terms</a></h5>\r\n\r\n<form class=\"form\" action=\"{{urls.subscribe.action}}\" method=\"post\">\r\n    <fieldset class=\"form-fieldset\">\r\n        <input type=\"hidden\" name=\"action\" value=\"subscribe\">\r\n        <input type=\"hidden\" name=\"nl_first_name\" value=\"bc\">\r\n        <input type=\"hidden\" name=\"check\" value=\"1\">\r\n        <div class=\"form-field\">\r\n            <label class=\"form-label is-srOnly\" for=\"nl_email\">{{lang 'common.email_address'}}</label>\r\n            <div class=\"form-prefixPostfix wrap\">\r\n                <div class=\"footer__input-wrapper\">\r\n                    <input class=\"form-input footer__name-input\" placeholder=\"Name\" type=\"name\" value=\"{{customer.name}}\">\r\n                    <input class=\"form-input footer__email-input\" id=\"nl_email\" name=\"nl_email\" type=\"email\" value=\"{{customer.email}}\"\r\n                    placeholder=\"Email\">\r\n                </div>\r\n\r\n                <div class=\"footer__submit-wrapper\">\r\n                    <input class=\"button button--primary form-prefixPostfix-button--postfix footer__subscribe-btn\" type=\"submit\"\r\n                    value=\"SUBMIT\">\r\n                </div>\r\n            {{#if settings.show_newsletter_summary }}\r\n            <div class=\"footer-newsletter-summary\">{{settings.newsletter_summary}}</div>\r\n            {{/if}}\r\n        </div>\r\n    </fieldset>\r\n</form>\n","components/common/payment-icons":"{{#with theme_settings}}\r\n    {{#or show_accept_amex show_accept_discover show_accept_mastercard show_accept_paypal show_accept_visa}}\r\n        <div class=\"footer-payment-icons\">\r\n            {{#if show_accept_amex}}\r\n                <svg class=\"footer-payment-icon\"><use xlink:href=\"#icon-logo-american-express\"></use></svg>\r\n            {{/if}}\r\n            {{#if show_accept_discover}}\r\n                <svg class=\"footer-payment-icon\"><use xlink:href=\"#icon-logo-discover\"></use></svg>\r\n            {{/if}}\r\n            {{#if show_accept_mastercard}}\r\n                <svg class=\"footer-payment-icon\"><use xlink:href=\"#icon-logo-mastercard\"></use></svg>\r\n            {{/if}}\r\n            {{#if show_accept_paypal}}\r\n                <svg class=\"footer-payment-icon\"><use xlink:href=\"#icon-logo-paypal\"></use></svg>\r\n            {{/if}}\r\n            {{#if show_accept_visa}}\r\n                <svg class=\"footer-payment-icon\"><use xlink:href=\"#icon-logo-visa\"></use></svg>\r\n            {{/if}}\r\n        </div>\r\n    {{/or}}\r\n{{/with}}\r\n","components/common/social-links":"{{#if social_media}}\r\n    <ul class=\"socialLinks socialLinks--alt\">\r\n        {{#each social_media}}\r\n            <li class=\"socialLinks-item\">\r\n                {{#if display_name '===' 'google+'}}\r\n                    <a class=\"icon icon--google\" href=\"{{url}}\" target=\"_blank\">\r\n                        <svg><use xlink:href=\"#icon-google\" /></svg>\r\n                    </a>\r\n                {{else}}\r\n                    <a class=\"icon icon--{{display_name}}\" href=\"{{url}}\" target=\"_blank\">\r\n                        <svg><use xlink:href=\"#icon-{{display_name}}\" /></svg>\r\n                    </a>\r\n                {{/if}}\r\n            </li>\r\n        {{/each}}\r\n    </ul>\r\n{{/if}}\r\n"}

{"pages/auth/new-password":"{{inject 'passwordRequirements' settings.password_requirements}}\r\n{{#partial \"page\"}}\r\n<div class=\"account account--fixedSmall\">\r\n    {{> components/common/breadcrumbs breadcrumbs=breadcrumbs}}\r\n    <h2 class=\"page-heading\">{{lang 'forms.new_password.heading' }}</h2>\r\n    <p>{{forms.new_password.requirements}}</p>\r\n    {{#if forms.new_password.error}}\r\n        {{> components/common/alert-error forms.new_password.error}}\r\n    {{/if}}\r\n    <form action=\"{{forms.new_password.submit_url}}\" class=\"form new-password-form\" method=\"post\">\r\n        <div class=\"form-row\">\r\n            <div class=\"form-field\">\r\n                <label class=\"form-label\" for=\"password\">\r\n                    {{lang 'forms.new_password.password' }}\r\n                    <small>{{lang 'common.required' }}</small>\r\n                </label>\r\n                <input type=\"password\" class=\"form-input\" name=\"password\" id=\"password\">\r\n            </div>\r\n            <div class=\"form-field\">\r\n                <label class=\"form-label\" for=\"password_confirm\">\r\n                    {{lang 'forms.new_password.password2' }}\r\n                    <small>{{lang 'common.required' }}</small>\r\n                </label>\r\n                <input type=\"password\" class=\"form-input\" name=\"password_confirm\" id=\"password_confirm\">\r\n            </div>\r\n        </div>\r\n        <div class=\"form-actions\">\r\n            <input\r\n                class=\"button button--primary\"\r\n                type=\"submit\"\r\n                value=\"{{lang 'forms.new_password.submit_value' }}\">\r\n        </div>\r\n    </form>\r\n</div>\r\n{{/partial}}\r\n{{> layout/base}}\r\n","components/common/alert-error":"<div class=\"alertBox alertBox--error\">\r\n    <div class=\"alertBox-column alertBox-icon\">\r\n        <icon glyph=\"ic-error\" class=\"icon\" aria-hidden=\"true\"><svg xmlns=\"http://www.w3.org/2000/svg\" width
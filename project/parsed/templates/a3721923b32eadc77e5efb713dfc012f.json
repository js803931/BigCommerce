{"components/products/options/set-rectangle":"<div class=\"form-field option--{{#replace ' ' (toLowerCase this.display_name)}}{{else}}{{toLowerCase this.display_name}}{{/replace}}\" data-product-attribute=\"set-rectangle\">\r\n    <label class=\"form-label form-label--alternate form-label--inlineSmall\">\r\n        {{this.display_name}}\r\n        <span data-option-value></span>\r\n        {{#if required}}\r\n            <small>{{lang 'common.required'}}</small>\r\n        {{/if}}\r\n    </label>\r\n    {{#each this.values}}\r\n        <input\r\n            class=\"form-radio\"\r\n            type=\"radio\"\r\n            id=\"attribute_rectangle_{{id}}\"\r\n            name=\"attribute[{{../id}}]\"\r\n            value=\"{{id}}\"\r\n            {{#if selected}}\r\n                checked\r\n                data-default\r\n            {{/if}}\r\n            {{#if ../required}}required{{/if}}>\r\n        <label class=\"form-option\" for=\"attribute_rectangle_{{id}}\" data-product-attribute-value=\"{{id}}\">\r\n            <span class=\"form-option-variant\">{{this.label}}</span>\r\n        </label>\r\n    {{/each}}\r\n\r\n    {{#if this.display_name '===' 'Size'}}\r\n        <div>\r\n            {{> components/custom/size-guide-modal }}\r\n        </div>\r\n    {{/if}}\r\n</div>\r\n","components/custom/size-guide-modal":"\r\n<a href=\"##\" class=\"size-guide-link\" data-reveal-id=\"size-guide-link\">{{lang 'custom.view-size-guide'}}</a>\r\n    \r\n    \r\n    <div id=\"size-guide-link\" class=\"modal modal--large size-guide-modal\" data-reveal>\r\n        <div class=\"modal-header\">\r\n            <h2 class=\"modal-header-title\">Size Guide</h2>\r\n            <a href=\"#\" class=\"modal-close\" aria-label=\"{{lang 'common.close'}}\" role=\"button\">\r\n                <span aria-hidden=\"true\">&#215;</span>\r\n            </a>\r\n        </div>\r\n        <div class=\"modal-body\">\r\n            <img class=\"lazyload size-guide-image\" data-src=\"{{cdn 'webdav:/images/default-size-guide.png'}}\" src=\"{{cdn 'img/loading.svg'}}\" alt=\"size guide\">\r\n        </div>\r\n    </div>\r\n   \n"}

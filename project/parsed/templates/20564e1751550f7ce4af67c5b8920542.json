{"components/custom/category-swatches":"{{#each product.options}}\n\t{{#if type '===' 'Configurable_PickList_Swatch'}}\n\t\t{{{dynamicComponent 'components/custom/options'}}}\n\t{{/if}}\n{{/each}}\n","components/custom/options/swatch":"<div class=\"card-option--{{id}} card-option__parent\">\n    {{#each (limit this.values 99)}}\n        {{#if @index '===' 3}}\n            <span class='form-option-variant card-option__option card-option__option--more' title=\"{{this.label}}\" \n            style=\"background:{{#if this.data}}#{{this.data}}{{else}}url({{getImage image 'core-swatch'}}){{/if}};\" \n            data-option-name=\"attribute[{{../../../id}}]\" data-option-value=\"{{id}}\"></span>\n            <a class=\"show-card-options\" href=\"##\">+ More</a>\n        {{else}}\n            {{#if @index '>' 3}}\n                <span class='form-option-variant card-option__option card-option__option--hidden' title=\"{{this.label}}\" \n                style=\"background:{{#if this.data}}#{{this.data}}{{else}}url({{getImage image 'core-swatch'}}){{/if}};\"\n                 data-option-name=\"attribute[{{../../../id}}]\" data-option-value=\"{{id}}\"></span>\n            {{else}}\n                <span class='form-option-variant card-option__option' title=\"{{this.label}}\" \n                style=\"background:{{#if this.data}}#{{this.data}}{{else}}url({{getImage image 'core-swatch'}}){{/if}};\" \n                data-option-name=\"attribute[{{../../../id}}]\" data-option-value=\"{{id}}\"></span>\n            {{/if}}\n        {{/if}}\n    {{/each}}\n</div>\n"}
